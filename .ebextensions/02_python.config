container_commands:
  01_migrate:
    command: ".venv\Scripts\activate && py manage.py migrate --noinput"
    leader_only: true

  02_createsu:
    command: ".venv\Scripts\activate && py manage.py createsu"
    leader_only: true

  03_collectstatic:
    command: ".venv\Scripts\activate && py manage.py collectstatic --noinput"